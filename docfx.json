{
  "metadata": [
    {
      "src": "src/**.dll",
      "dest": "api",
      "filter": "filterapi.yml"
    }
  ],
  "build": {
    "sitemap": {
        "baseUrl": "http://wellosoft.net/engine4-doc",
        "changefreq": "monthly",
        "lastmod": "2017-11-14T04:58:38+07:00"
    },
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "manual/**.md",
          "manual/**/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "template": [
      "default",
      "template"
    ],
    "globalMetadata": {
      "_appTitle": "Engine4 Manual",
      "_enableSearch": true,
      "_disableBreadcrumb": true,
      "_disableAffix": true,
      "_disableContribution": true,
      "_appLogoPath": "images/icon_small.png",
      "_gitContribute": {
        "repo": "https://github.com/willnode/engine4-src",
        "branch": "master",
        "apiSpecFolder": "docs-ref-overwrite"
      }
    },
    "postProcessors": [],
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false
  }
}
